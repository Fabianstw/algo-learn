{
  "name": "algo-learn",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@3.5.0",
  "engines": {
    "node": ">=18.0.0 <20.0.0"
  },
  "license": "MIT",
  "author": "Holger Dell (https://holgerdell.com)",
  "scripts": {
    "start": "$npm_execpath run dev",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.[t|j]s src/**/*.[t|j]sx",
    "check": "tsc",
    "format": "prettier src/ --write",
    "pre-commit": "$npm_execpath run prettier src/ --check && $npm_execpath run check && $npm_execpath run test && $npm_execpath run lint",
    "clean": "rm -rf dist/ .yarn/cache/ node_modules/ docs/ vite-bundle-visualizer.html",
    "postinstall": "husky install",
    "visualize": "$npm_execpath run vite-bundle-visualizer -o vite-bundle-visualizer.html",
    "docs": "$npm_execpath run typedoc --out docs src/**/*.ts src/**/*.tsx"
  },
  "devDependencies": {
    "@types/katex": "^0.16.0",
    "@types/node": "^18.15.13",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "pinst": "^3.0.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.7",
    "prettier-plugin-css-order": "^1.3.0",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "react-circular-progressbar": "^2.1.0",
    "stylelint": "^15.6.0",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.3.1",
    "typedoc": "^0.24.6",
    "typescript": "^5.0.4",
    "vite": "^4.3.1",
    "vite-bundle-visualizer": "^0.6.0",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "@fontsource/noto-sans": "^4.5.11",
    "@matejmazur/react-katex": "^3.1.3",
    "fraction.js": "^4.2.0",
    "i18next": "^22.4.15",
    "i18next-browser-languagedetector": "^7.0.1",
    "katex": "^0.16.6",
    "mathjs": "^11.8.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.10.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-tooltip": "^5.11.1",
    "use-local-storage-state": "^18.3.2"
  }
}
