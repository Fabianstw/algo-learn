{
  "name": "algo-learn",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@3.5.1",
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT",
  "author": "Holger Dell (https://holgerdell.com)",
  "scripts": {
    "dev": "vite front-end",
    "build": "vite build front-end",
    "preview": "vite preview front-end",
    "build:json-schema": "$npm_execpath run ts-json-schema-generator --path 'src/**/QuestionGenerator.ts' --tsconfig ./tsconfig.ts-json-schema-generator.json --type 'Question'",
    "test": "$npm_execpath run test:format && $npm_execpath run test:check && $npm_execpath run test:unit && $npm_execpath run test:lint",
    "test:unit": "vitest run",
    "test:lint": "eslint */src/**/*.ts */src/**/*.tsx",
    "test:check": "tsc",
    "test:format": "prettier front-end/src/ shared/src --check",
    "format": "prettier front-end/src/ shared/src */src/**/*.css --write",
    "clean": "rm -rf dist/ .yarn/cache/ node_modules/ docs/ vite-bundle-visualizer.html",
    "visualize": "$npm_execpath run vite-bundle-visualizer -o vite-bundle-visualizer.html",
    "docs": "$npm_execpath run typedoc --out docs shared/src/**/*.ts shared/src/**/*.tsx"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@types/katex": "^0.16.0",
    "@types/node": "^18.16.9",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.40.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "pinst": "^3.0.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-css-order": "^1.3.0",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "react-circular-progressbar": "^2.1.0",
    "stylelint": "^15.6.1",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.3.2",
    "ts-json-schema-generator": "^1.2.0",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.0 <5.1.0",
    "vite": "^4.3.5",
    "vite-bundle-visualizer": "^0.6.0",
    "vitest": "^0.31.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "@fontsource/noto-sans": "^4.5.11",
    "@matejmazur/react-katex": "^3.1.3",
    "fraction.js": "^4.2.0",
    "i18next": "^22.4.15",
    "i18next-browser-languagedetector": "^7.0.1",
    "katex": "^0.16.7",
    "mathjs": "^11.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.2",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.11.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-tooltip": "^5.11.2",
    "use-local-storage-state": "^18.3.2"
  }
}
